{
  "private": true,
  "name": "{{cookiecutter.extension_name}}_nbextension",
  "version": "0.20.2",
  "description": "A Jupyter Notebook extension for rendering {{cookiecutter.mime_short_name}}",
  "author": "{{cookiecutter.author_name}} <{{cookiecutter.author_email}}>",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension"
  ],
  "scripts": {
    "build": "webpack",
    "prepublishOnly": "npm run build",
    "watch": "watch \"npm run build\" src --wait 10 --ignoreDotFiles",
    "extension:install": "jupyter nbextension install --symlink --py --sys-prefix {{cookiecutter.extension_name}}",
    "extension:uninstall": "jupyter nbextension uninstall --py --sys-prefix {{cookiecutter.extension_name}}",
    "extension:enable": "jupyter nbextension enable --py --sys-prefix {{cookiecutter.extension_name}}",
    "extension:disable": "jupyter nbextension disable --py --sys-prefix {{cookiecutter.extension_name}}"
  },
  "dependencies": {
    "{{cookiecutter.extension_name}}_react": "file:../component"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.4.0",
    "babel-preset-latest": "^6.16.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^1.0.1",
    "webpack": "^2.2.0"
  }
}
